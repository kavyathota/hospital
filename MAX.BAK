#include<iostream.h>
#include<conio.h>
template<class t>
t max(t a,t b)
{
if(a>b)
return a;
else
return b;
}
void main()
{
char c1,c2,c3;
cout<<"enter two characters"<<endl;
cin>>c1>>c2;
c3=max(c1,c2);
